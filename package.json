{
  "name": "nuxt-error-and-cache",
  "type": "module",
  "version": "1.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xjccc/nuxt-error-and-cache.git"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "echo:husky": "echo 'npm run lint' > .husky/pre-commit && echo 'npx --no-install commitlint --edit \"$1\"' > .husky/commit-msg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky && pnpm echo:husky",
    "commit": "git-cz",
    "release": "changelogen --release && pnpm prepack && npm publish && git push --follow-tags"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "lru-cache": "^10.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxt/module-builder": "^0.7.0",
    "@nuxt/schema": "^3.11.2",
    "@types/glob": "^8.1.0",
    "changelogen": "^0.5.5",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.2",
    "eslint": "^9.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nuxt": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
